{
  "name": "cars-tournaments-app",
  "version": "0.2.8",
  "author": "JoseXS",
  "description": "CarsTournaments",
  "homepage": "https://carstournaments.com",
  "repository": "https://github.com/josexs/carsTournaments-app.git",
  "bugs": {
    "url": "https://github.com/josexs/carsTournaments-app/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "test": "ng test --code-coverage",
    "test:no-watch": "ng test --no-watch --code-coverage",
    "e2e": "ng e2e",
    "lint": "eslint \"**/*.{ts,tsx}\" --fix",
    "prettier:list": "prettier --list-different \"./{e2e,src}/**/*.{scss,ts}\" || ECHO .",
    "prettier:write": "prettier --write \"./{e2e,src}/**/*.{scss,ts}\"",
    "prepare": "husky install",
    "generate:apk": "npm run build:prod && npx cap sync && cd android && ./gradlew assembleDebug",
    "generate:apk:release": "npm run build:prod && npx cap sync && cd android && ./gradlew assembleRelease",
    "generate:apk:run": "ionic capacitor run android --prod",
    "generate:bundle": "npm run build:prod && npx cap sync && cd android && ./gradlew bundleRelease",
    "livereload:android": "ionic capacitor run android --livereload --external",
    "livereload:ios": "ionic capacitor run ios --livereload --external",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer www/stats.json",
    "sonar": "sonar-scanner",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~13.2.2",
    "@angular/core": "~13.2.2",
    "@angular/forms": "~13.2.2",
    "@angular/platform-browser": "~13.2.2",
    "@angular/platform-browser-dynamic": "~13.2.2",
    "@angular/router": "~13.2.2",
    "@angular/service-worker": "~13.2.2",
    "@capacitor-community/admob": "^3.2.0",
    "@capacitor-community/firebase-analytics": "~1.0.1",
    "@capacitor/android": "3.4.3",
    "@capacitor/app": "1.1.1",
    "@capacitor/browser": "~1.0.7",
    "@capacitor/camera": "~1.3.1",
    "@capacitor/configure": "~2.0.8",
    "@capacitor/core": "3.4.3",
    "@capacitor/haptics": "1.1.4",
    "@capacitor/ios": "3.4.3",
    "@capacitor/keyboard": "1.2.2",
    "@capacitor/push-notifications": "^1.0.9",
    "@capacitor/share": "^1.1.2",
    "@capacitor/splash-screen": "^1.2.2",
    "@capacitor/status-bar": "1.0.8",
    "@capgo/capacitor-updater": "^3.3.6",
    "@codetrix-studio/capacitor-google-auth": "^3.1.3",
    "@compodoc/compodoc": "^1.1.19",
    "@fortawesome/angular-fontawesome": "^0.10.2",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@ionic/angular": "~6.0.0",
    "@ionic/pwa-elements": "~3.1.1",
    "@ionic/storage-angular": "3.0.6",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "animate.css": "~4.1.1",
    "moment": "~2.29.1",
    "ng2-adsense": "^10.1.0",
    "rxjs": "~6.6.0",
    "storage": "0.2.0",
    "swiper": "~8.1.0",
    "tslib": "~2.2.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.3",
    "@angular-eslint/builder": "~13.0.1",
    "@angular-eslint/eslint-plugin": "~13.0.1",
    "@angular-eslint/eslint-plugin-template": "~13.0.1",
    "@angular-eslint/template-parser": "~13.0.1",
    "@angular/cli": "~13.2.3",
    "@angular/compiler": "~13.2.2",
    "@angular/compiler-cli": "~13.2.2",
    "@angular/language-service": "~13.2.2",
    "@capacitor/cli": "3.4.3",
    "@ionic/angular-toolkit": "~6.0.0",
    "@ionic/cli": "6.19.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.11.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "auto-changelog": "^2.4.0",
    "commitlint": "^17.0.3",
    "cordova-res": "0.15.4",
    "eslint": "~7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "husky": "^8.0.1",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.5.0",
    "prettier": "~2.6.0",
    "pretty-quick": "~3.1.3",
    "ts-node": "~8.3.0",
    "typescript": "~4.4.4"
  },
  "eslintIgnore": [
    "*.spec.ts"
  ],
  "keywords": [
    "CarsTournaments",
    "Angular",
    "Ionic",
    "Capacitor",
    "SCSS",
    "HTML",
    "Javascript",
    "Typescript"
  ]
}
