{
  "name": "cars-tournaments-app",
  "version": "0.5.0",
  "author": "JoseXS",
  "description": "CarsTournaments",
  "homepage": "https://carstournaments.com",
  "repository": "https://github.com/josexs/carsTournaments-app.git",
  "bugs": {
    "url": "https://github.com/josexs/carsTournaments-app/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ionic serve",
    "start:prod": "ionic serve --configuration=production",
    "build": "ng build",
    "build:prod": "ng build --configuration=production",
    "test": "ng test --code-coverage",
    "test:no-watch": "ng test --no-watch --code-coverage",
    "e2e": "npx cypress run --config-file cypress.config.ts --browser chrome",
    "e2e:cloud": "npx cypress run --config-file cypress.config.ts --browser chrome --record --key d3541dc8-24d8-46be-a5fd-51e154281268",
    "lint": "eslint \"**/*.{ts,tsx}\" --fix",
    "prettier:list": "prettier --list-different \"./{e2e,src}/**/*.{scss,ts}\" || ECHO .",
    "prettier:write": "prettier --write \"./{e2e,src}/**/*.{scss,ts}\"",
    "prepare": "husky install",
    "generate:apk": "npm run build:prod && npx cap sync && cd android && ./gradlew assembleDebug",
    "generate:aab": "npm run build:prod && npx cap sync && cd android && ./gradlew bundle",
    "generate:apk:release": "npm run build:prod && npx cap sync && cd android && ./gradlew assembleRelease",
    "generate:apk:run": "ionic capacitor run android --prod",
    "generate:bundle": "npm run build:prod && npx cap sync && cd android && ./gradlew bundleRelease",
    "livereload:android": "ionic capacitor run android --livereload --external",
    "livereload:ios": "ionic capacitor run ios --livereload --external",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer www/stats.json",
    "compodoc": "compodoc -p tsconfig.app.json",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "dependencies": {
    "@angular/common": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@angular/service-worker": "^14.1.0",
    "@capacitor-community/admob": "4.0.0-1",
    "@capacitor-firebase/analytics": "^0.5.1-dev.bd42e0a.1659614749",
    "@capacitor-firebase/authentication": "^0.5.1-dev.bd42e0a.1659614749",
    "@capacitor/android": "4.0.1",
    "@capacitor/app": "4.0.1",
    "@capacitor/browser": "4.0.1",
    "@capacitor/camera": "4.0.1",
    "@capacitor/configure": "2.0.10",
    "@capacitor/core": "4.0.1",
    "@capacitor/device": "4.0.1",
    "@capacitor/haptics": "4.0.1",
    "@capacitor/ios": "4.0.1",
    "@capacitor/keyboard": "4.0.1",
    "@capacitor/push-notifications": "4.0.1",
    "@capacitor/share": "4.0.1",
    "@capacitor/splash-screen": "4.0.1",
    "@capacitor/status-bar": "4.0.1",
    "@capawesome/capacitor-app-update": "^2.0.0",
    "@compodoc/compodoc": "^1.1.19",
    "@fortawesome/angular-fontawesome": "0.11.1",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@ionic/angular": "~6.1.9",
    "@ionic/pwa-elements": "~3.1.1",
    "@ionic/storage-angular": "3.0.6",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "animate.css": "~4.1.1",
    "lint-staged": "^13.0.3",
    "moment": "~2.29.1",
    "ng2-adsense": "^10.1.0",
    "rxjs": "~6.6.0",
    "storage": "0.2.0",
    "swiper": "~8.1.0",
    "tslib": "~2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.0",
    "@angular-eslint/builder": "14.0.2",
    "@angular-eslint/eslint-plugin": "14.0.2",
    "@angular-eslint/eslint-plugin-template": "14.0.2",
    "@angular-eslint/template-parser": "14.0.2",
    "@angular/cli": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/compiler-cli": "^14.1.0",
    "@angular/language-service": "^14.1.0",
    "@capacitor/cli": "4.0.1",
    "@ionic/angular-toolkit": "7.0.0",
    "@ionic/cli": "6.20.1",
    "@types/cypress": "^1.1.3",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.11.1",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "auto-changelog": "^2.4.0",
    "commitlint": "^17.0.3",
    "cordova-res": "0.15.4",
    "cypress": "^10.3.1",
    "cypress-file-upload": "^5.0.8",
    "eslint": "~7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "husky": "^8.0.1",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.5.0",
    "prettier": "2.7.1",
    "pretty-quick": "~3.1.3",
    "ts-node": "~8.3.0",
    "typescript": "~4.7.4",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "eslintIgnore": [
    "*.spec.ts"
  ],
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "keywords": [
    "CarsTournaments",
    "Angular",
    "Ionic",
    "Capacitor",
    "SCSS",
    "HTML",
    "Javascript",
    "Typescript"
  ],
  "jshintConfig": {
    "esversion": 6
  }
}
